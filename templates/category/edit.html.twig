{% extends "base.html.twig" %}

{% block title %}
	Edition de
	{{ category.name }}
{% endblock %}

{% block body %}
	<h1>Editer la catégorie:
		{{ category.name }}</h1>

	{{ form_start(formView)}}

	<div class="row">
		<div class="col">
			{{ form_row(formView.name) }}
		</div>
	</div>

	<button class="btn btn-primary" type="submit">
		<i class="fas fa-save"></i>
		Crée le produit</button>
	{{ form_end(formView) }}

{% endblock %}
